generator clientLegacy {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/client-legacy"
}

datasource dbLegacy {
  provider = "mysql"
  url      = env("DATABASE_URL_LEGACY")
}

model acuerdos {
  id                               Int             @id @default(autoincrement()) @dbLegacy.UnsignedInt
  fecha                            DateTime        @dbLegacy.Date
  descripcion                      String          @dbLegacy.LongText
  estado                           acuerdos_estado @default(Abierto)
  credito_id                       Int             @dbLegacy.UnsignedInt
  created_by                       Int             @dbLegacy.UnsignedInt
  updated_by                       Int?            @dbLegacy.UnsignedInt
  created_at                       DateTime?       @dbLegacy.Timestamp(0)
  updated_at                       DateTime?       @dbLegacy.Timestamp(0)
  users_acuerdos_created_byTousers users           @relation("acuerdos_created_byTousers", fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "acuerdos_created_by_foreign")
  creditos                         creditos        @relation(fields: [credito_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "acuerdos_credito_id_foreign")
  users_acuerdos_updated_byTousers users?          @relation("acuerdos_updated_byTousers", fields: [updated_by], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "acuerdos_updated_by_foreign")

  @@index([created_by], map: "acuerdos_created_by_foreign")
  @@index([credito_id], map: "acuerdos_credito_id_foreign")
  @@index([updated_by], map: "acuerdos_updated_by_foreign")
}

